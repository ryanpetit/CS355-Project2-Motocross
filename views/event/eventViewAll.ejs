<%- include("../includes/header.ejs", {title:"Events - View All"}); %>
<div class="center">
<h1>Events</h1>

<table border="2", align="center">
    <tr><th>Name</th></tr>
    <% for(var i=0; result.length > i; i++ ) { %>
    <tr>
        <td><a href="/event/?Event_ID=<%= result[i].Event_ID %>" style="color: #d200ff;"><%= result[i].Event_Name%> </a></td>
    </tr>
    <% } %>
</table>
<br />
<a href="/" style="color: #0002ff">Back to Main Page</a>
</body>
</div>
